      "settings": {
        "foreground": "#075985"
      }
    },
    {
      "scope": "text.html.markdown",
      "settings": {
        "foreground": "#7c3aed"
      }
    },
    {
      "name": "[VSCODE-CUSTOM] Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "Support",
      "scope": "support.function",
      "settings": {
        "foreground": "#3730a3"
      }
    }
